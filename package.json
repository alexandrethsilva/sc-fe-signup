    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
