    "eslint-config-standard-pure-fp": "2.0.1",
    "eslint-plugin-better": "0.1.5",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-xod-fp": "0.1.1",
    "express": "4.16.2",
    "flow-bin": "0.57.2",
    "jsverify": "0.8.3",
    "meow": "3.7.0",
    "redent": "2.0.0",
    "request-promise": "4.2.2",
    "source-map-loader": "0.2.2",
    "strip-indent": "2.0.0",
    "to-string-loader": "1.1.5",
    "webpack": "3.7.1",
    "xo": "0.18.2"
  },
  "ava": {
    "babelrc": true,
    "require": [
      "babel-register"
    ],
    "verbose": false
  },
  "xo": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "plugins": [
      "better",
      "fp",
      "import",
      "promise",
      "standard",
      "xod-fp"
    ],
    "extends": "standard-pure-fp",
    "rules": {
      "capitalized-comments": [
        "off"
      ],
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "indent": [
        "warn"
      ],
      "better/no-ifs": [
        "warn"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "import/no-commonjs": [
        "off",
        "allow-primitive-modules"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/no-unresolved": [
        "off"
      ],
      "import/order": [
        "error"
      ],
      "fp/no-mutation": [
        "error",
        {
          "commonjs": true,
          "allowThis": true
        }
      ]
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "plugins": [
      "better",
      "fp",
      "import",
      "promise",
      "standard",
      "xod-fp"
    ],
    "extends": "standard-pure-fp",
    "rules": {
      "capitalized-comments": [
        "off"
      ],
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "indent": [
        "warn"
      ],
      "better/no-ifs": [
        "warn"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "import/no-commonjs": [
        "off",
        "allow-primitive-modules"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/no-unresolved": [
        "off"
      ],
      "import/order": [
        "error"
      ],
      "fp/no-mutation": [
        "error",
        {
          "commonjs": true,
          "allowThis": true
        }
      ]
    }
  },
  "private": true,
  "resolutions": {
    "ramda": "0.24.1",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
